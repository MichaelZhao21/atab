{
    "manifest_version": 2,
    "name": "aTab",
    "version": "1.1",
    "author": "Michael Zhao",
    "description": "An extension that will replace your new tab with a more productive and useful interface!",
    
    "icons": {
        "48": "res/48.png",
        "96": "res/96.png"
    },

    "chrome_settings_overrides": {
		"homepage": "dist/index.html"
	},
	"chrome_url_overrides": {
		"newtab": "dist/index.html"
	},

    "permissions": ["<all_urls>", "storage"],

	"browser_specific_settings": {
		"gecko": {
			"id": "atab@michaelzhao.xyz",
			"strict_min_version": "70.0"
		}
	}
}